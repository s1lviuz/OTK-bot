{
  "name": "otk-bot",
  "version": "1.0.0",
  "description": "Um bot do Discord da Nação OTK",
  "main": "index.ts",
  "scripts": {
    "start": "nodemon src/index.ts",
    "dev": "nodemon src/index.ts . | pino-pretty -i pid,hostname -t 'dd-mm-yyyy HH:MM:ss'",
    "deploy": "ts-node src/deploy-commands.ts . | pino-pretty -i pid,hostname -t 'dd-mm-yyyy HH:MM:ss'",
    "remove": "ts-node src/delete-commands.ts . | pino-pretty -i pid,hostname -t 'dd-mm-yyyy HH:MM:ss'"
  },
  "keywords": [
    "discord",
    "node"
  ],
  "author": "S1lviuz",
  "license": "ISC",
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "axios": "^1.2.0",
    "common-tags": "^1.8.2",
    "dayjs": "^1.11.6",
    "discord.js": "^14.6.0",
    "dotenv": "^16.0.3",
    "ms": "^2.1.3",
    "pino": "^8.7.0"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.1",
    "@types/node": "^18.11.10",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
}
